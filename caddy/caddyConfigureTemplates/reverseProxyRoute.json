{
    "match": [{"path":["/identities"]},{"path":["/identitiesLogs"]}],
    "handle": [{
        "handler": "reverse_proxy",
        "upstreams": [{
            "dial": "127.0.0.1:1572"
        }],
        "headers": {
            "request": {
                "add": {"x-forwarded-for": ["{http.request.remote.host}"]}
            },
            "response":{
                "add": {
                    "Access-Control-Allow-Origin": ["*"],
                    "Access-Control-Allow-Methods": ["*"],
                    "Access-Control-Allow-Headers": ["*"]
                }
            }
        }
    }]
}